@using Travellers.Entities.Concrete.Dtos;

@{
    ViewData["Title"] = "Add";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model TourDto

<form asp-action="Add" asp-controller="Tour" method="post">
    <h1>Tour Add</h1>
    <label>Category Id:</label>
    <input type="text" name="categoryId" /> <br />
    <label>Tour Name:</label>
    <input type="text" name="tourName" /> <br />
    <label>Image:</label>
    <input type="text" name="image" /> <br />
    <input type="submit" value="Add" />
</form>




